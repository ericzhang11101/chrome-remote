{
  "manifest_version": 3,
  "name": "placeholder name",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "placeholder.html",
    "default_icon": "images/icon.png"
  },
  "content_scripts": [
    {
      "js": ["./bundle/content.js"],
      "matches": [
        "https://www.youtube.com/*"
      ],
      "type": "module"
    },
    {
      "css": ["./bundle/styles/style.css"],
      "matches": [
        "https://www.youtube.com/*"
      ]
    }
  ],
  "background": {
    "type": "module"
  }
}